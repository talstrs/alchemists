											<div class="alc-player-info__name">
												{{player.name}}
											</div>
										</div>
									</td>

									{{#each stats}}
									<td>{{value}}</td>
									{{/each}}

									<td>{{#if icon}}<i class="icon-svg {{icon}}"></i>{{/if}}</td>
								</tr>
								{{/each}}
								<!-- Played / End -->

							</tbody>
						</table>

					</div>
				</div>
				{{/each}}
				<!-- Stats / End -->

			</div>
			<div class="col-lg-4">

				<!-- Widget: Lineup -->
				<div class="widget card widget--sidebar widget-lineup">
					<div class="widget__title card__header">
						<h4>Clovers Tactic</h4>
					</div>
					<div class="widget__content card__content">

						<div class="alc-formation">
							<ul class="alc-formation__field alc-formation__field--lineup-2">
								{{#each soccer.sp.event.lineups.team2}}
								<li class="alc-formation__player">
									<div class="alc-formation__player-tshirt alc-formation__player-tshirt--alt {{#if gp}}alc-formation__player-tshirt--clean{{/if}}">
										<div class="alc-formation__player-number">{{number}}</div>
									</div>
									<span class="alc-formation__player-name">{{name}}</span>
								</li>
								{{/each}}
							</ul>
						</div>

					</div>
				</div>
				<!-- Widget: Lineup / End -->

			</div>
		</div>
		<!-- Alchemists - Stats & Lineup / End -->
	</div>
</div>

<!-- Content / End -->
