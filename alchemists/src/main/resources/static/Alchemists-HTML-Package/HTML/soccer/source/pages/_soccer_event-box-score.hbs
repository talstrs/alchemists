								</tr>
								{{/each}}
								<!-- Played / End -->

								<!-- Staff -->
								{{#each team2.tbody.staff}}
								<tr>
									<td class="alc-align-start" colspan="2">{{position}}</td>
									<td class="alc-align-start">
										<div class="alc-player-info">
											<img src="{{person.img}}" class="alc-player-info__img" alt="{{person.name}}">
											<div class="alc-player-info__name">
												{{person.name}}
											</div>
										</div>
									</td>
									<td>-</td>
									<td>-</td>
									<td>-</td>
									<td>-</td>
									<td>-</td>
									<td>-</td>
									<td>-</td>
									<td>-</td>
									<td></td>
								</tr>
								{{/each}}
								<!-- Staff / End -->

								<tr>
									<th class="alc-subheader alc-highlight" colspan="12">Substitute Players</th>
								</tr>

								<!-- Substitute -->
								{{#each team2.tbody.substitute}}
								<tr>
									<td class="alc-number alc-align-start">{{number}}</td>
